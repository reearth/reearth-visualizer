REEARTH_WEB_E2E_BASEURL=
REEARTH_E2E_EMAIL=
REEARTH_E2E_PASSWORD=

# Google IAP Authentication (optional)
# Set USE_IAP_AUTH=true to enable IAP authentication for protected environments
# If not set, it will auto-detect based on URL (enabled for dev/staging, disabled for localhost/production)
USE_IAP_AUTH=
# IAP authentication method: 'adc' (Application Default Credentials), 'service-account', or 'id-token'
IAP_AUTH_METHOD=adc
# IAP target audience (OAuth client ID for the IAP-protected resource)
IAP_TARGET_AUDIENCE=
# Google Service Account JSON (required for 'service-account' method)
GOOGLE_SERVICE_ACCOUNT_JSON=
